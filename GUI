        "### **Data Exploration GUI**\n",
        "\n",
        "Filter the Chicago crime dataset based on the type of crime and month. "
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 925,
          "referenced_widgets": [
            "056a3e7e06c2486bbfd9d3e03fdf8ca5",
            "d4a53e7e863249469406019cc7fee302",
            "9fbc2e4438144d05ad203717e1156387",
            "96eaac7228d14b55972263cfacb4d108",
            "6f8611e7e14641c1b054c5b9ecd5cade",
            "24ce0907a6df4233b58c63c164cd7ae5",
            "be11cd87cd694cd497a23dcbef2859bf",
            "41442a5d2aea42f5b56b7d3eb687e5c1",
            "7a0ed314767e49ae884a4bb0d147ade3",
            "a1bbf47831b64212a1c1ae2f7fb9a015",
            "f5ec3a85674c478b8bfe1f5ed3133b58",
            "aff7da609a4e4739aa54925b475b5a60",
            "b2e1baed990b4700aa7d4ec7174dcc20"
          ]
        },
        "id": "_ehgXLlq_REZ",
        "outputId": "1f0e9ab4-9b0e-4cfc-f7f2-9e05f4225809"
      },
      "source": [
        "import ipywidgets as widgets\n",
        "from IPython.display import display \n",
        "\n",
        "chicago_output = widgets.Output()\n",
        "dc_output = widgets.Output() \n",
        "\n",
        "all = \"All\"\n",
        "\n",
        "def unique_sorted_all(array):\n",
        "  values = array.unique().tolist()\n",
        "  values.sort()\n",
        "  values.insert(0, all)\n",
        "  return values\n",
        "\n",
        "crime_type_dropdown = widgets.Dropdown(options = unique_sorted_all(new_chicago_df.crime_type), description = \"Crime Type:\") \n",
        "month_dropdown = widgets.Dropdown(options = unique_sorted_all(new_chicago_df.month), description = \"Month:\")\n",
        "\n",
        "def filter(crime_type, month):\n",
        "  chicago_output.clear_output()\n",
        "  dc_output.clear_output()\n",
        "\n",
        "  if (crime_type == all) & (month == all):\n",
        "    common_filter = new_chicago_df\n",
        "  elif (crime_type == all):\n",
        "    common_filter = new_chicago_df[new_chicago_df.month == month]\n",
        "  elif (month == all):\n",
        "    common_filter = new_chicago_df[new_chicago_df.crime_type == crime_type] \n",
        "  else:\n",
        "    common_filter = new_chicago_df[(new_chicago_df.crime_type == crime_type) & (new_chicago_df.month == month)] \n",
        "\n",
        "  with chicago_output:\n",
        "    display(common_filter)\n",
        "\n",
        "def crime_type_dropdown_event(change):\n",
        "  filter(change.new, month_dropdown.value) \n",
        "\n",
        "def month_dropdown_event(change):\n",
        "  filter(crime_type_dropdown.value, change.new) \n",
        "\n",
        "crime_type_dropdown.observe(crime_type_dropdown_event, names = \"value\")\n",
        "month_dropdown.observe(month_dropdown_event, names = \"value\")\n",
        "\n",
        "item_layout = widgets.Layout(margin = '0 0 50px 0')\n",
        "input_widgets = widgets.HBox([crime_type_dropdown, month_dropdown],\n",
        "                             layout = item_layout)\n",
        "tab = widgets.Tab([chicago_output], \n",
        "                  layout = item_layout)\n",
        "tab.set_title(0, \"Chicago Dataset\")\n",
        "dashboard = widgets.VBox([input_widgets, tab])\n",
        "display(dashboard) "
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "application/vnd.jupyter.widget-view+json": {
              "model_id": "056a3e7e06c2486bbfd9d3e03fdf8ca5",
