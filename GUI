        "variable_dropdown = widgets.Dropdown(options = unique_sorted_all(new_chicago_df.columns), description = \"Variable:\")\n",
        "\n",
        "barplot_output = widgets.Output()\n",
        "piechart_output = widgets.Output() \n",
        "\n",
        "def variable_dropdown_event(variable):\n",
        "  barplot_output.clear_output() \n",
        "  piechart_output.clear_output() \n",
        "\n",
        "  with barplot_output:\n",
        "\n",
        "    if (variable.new == \"crime_type\"):\n",
        "      plt.subplots(figsize=(20, 8))\n",
        "      plt.subplot(1, 2, 1)\n",
        "      counts = new_chicago_df[\"crime_type\"].value_counts().head(10)\n",
        "      counts.sort_values().plot.barh(y = \"crime_type\", color = \"darkseagreen\")\n",
        "      plt.style.use(\"fivethirtyeight\")\n",
        "      plt.title(\"Categories of Offenses in Chicago\") \n",
        "\n",
        "      plt.subplot(1, 2, 2)\n",
        "      crime_counts = dc_df[\"crime_type\"].value_counts().head(10)\n",
        "      crime_counts.sort_values().plot.barh(\"crime_type\", color = \"springgreen\")\n",
        "      plt.title(\"Categories of Offenses in DC\")\n",
        "      plt.tight_layout() \n",
        "      plt.show()\n",
        "\n",
        "    elif (variable.new == \"arrest\"):\n",
        "      plt.figure(figsize = (9, 7)) \n",
        "      counts = new_chicago_df[\"arrest\"].value_counts()\n",
        "      counts.plot.bar(\"arrest\", color = \"yellowgreen\") \n",
        "      plt.style.use(\"fivethirtyeight\")\n",
