    {
      "cell_type": "markdown",
      "metadata": {
        "id": "zHp9uBZ_oYHc"
      },
      "source": [
        "## **Chicago Dataset**"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "fUZNm-EgcXfx",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 203
        },
        "outputId": "4d488441-1510-40f9-f4a7-df8b7c94551c"
      },
      "source": [
        "chicago_df = pd.read_csv(\"https://raw.githubusercontent.com/shoehair/beginner-track/main/CrimesChicago2018.csv\") \n",
        "\n",
        "#Adding date, day and months columns\n",
        "chicago_df[\"Date\"] = pd.to_datetime(chicago_df[\"Date\"])\n",
        "chicago_df[\"Day\"] = chicago_df[\"Date\"].dt.strftime(\"%A\")\n",
        "chicago_df[\"Month\"] = chicago_df[\"Date\"].dt.strftime(\"%B\")\n",
        "\n",
        "#Replacing NaN values with 0\n",
        "chicago_df = chicago_df.fillna(0)\n",
        "\n",
        "#Renaming columns\n",
        "chicago_df.columns = chicago_df.columns.str.lower() \n",
        "chicago_df = chicago_df.rename(columns = {\"case number\": \"case_number\", \"primary type\": \"crime_type\", \"description\": \"crime_description\", \"location description\": \"location_type\", \"community area\": \"area_code\", \n",
        "                                          \"fbi code\": \"fbi_code\", \"x coordinate\": \"x_coordinate\", \"y coordinate\": \"y_coordinate\", \"updated on\": \"last_updated\"}) \n",
        "\n",
        "#Dropping columns\n",
        "new_chicago_df = chicago_df.drop(columns = [\"beat\", \"year\", \"last_updated\", \"x_coordinate\", \"y_coordinate\", \"location\"])\n",
        "\n",
        "new_chicago_df.head(3)"
      ],
